# This is Git's per-user configuration file.
[user]
	name = Jessica Rehak
	email = rehak@lbl.gov
	signingkey = D4F1D1F6AE94C6D9
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	po = push origin
	poh = push origin HEAD
	unstage = reset HEAD --
	last = log -1 HEAD
	logg = log --graph --pretty=short --all --abbrev-commit --oneline
	graph = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all --use-mailmap
[color]
	ui = auto
[core]
	excludesfile = ~/.global-gitignore
	editor = vim

[init]
	defaultBranch = main
[credential]
	helper = manager
[commit]
	gpgsign = true
[tag]
	gpgSign = true
